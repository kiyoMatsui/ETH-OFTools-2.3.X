FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType  LES;

LES
{
    LESModel        Smagorinsky;

    turbulence      on;

    printCoeffs     on;

    delta           cubeRootVol;


SmagorinskyCoeffs
{
     Ck                  0.094;
     Ce                  1.048;
}

cubeRootVolCoeffs
{
    deltaCoeff      1;
}



